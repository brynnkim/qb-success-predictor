{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug QB Success (Integrated Terminal)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/qb_success.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/src",
      "environment": [],
  "externalConsole": true,
      "preLaunchTask": "build qb_success",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/Program Files/mingw64/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
